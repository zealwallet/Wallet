{
  "name": "top-up-dapp",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "build:dev": "ZEAL_ENV=development webpack --config webpack.config.js",
    "build:prod": "ZEAL_ENV=production webpack --config webpack.config.js",
    "start": "webpack serve --config webpack.config.js",
    "lint": "yarn eslint  --max-warnings 0 ./src",
    "inject-sourcemaps-debug-id": "yarn sentry-cli sourcemaps inject ./build",
    "upload-sourcemaps": "yarn sentry-cli sourcemaps upload --org ${SENTRY_ORG} --project ${SENTRY_PROJECT_TOP_UP_DAPP} --release $(node -p \"require('./package.json').version\") ./build",
    "run-ts": "ts-node --compiler-options '{\"types\": [\"node\"],\"module\": \"commonjs\", \"isolatedModules\": false}'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "eslint --max-warnings 0"
    ]
  },
  "dependencies": {
    "@sentry/react": "7.107.0",
    "@sentry/tracing": "7.107.0",
    "@tanstack/react-query": "5.0.5",
    "@zeal/api": "workspace:*",
    "@zeal/domains": "workspace:*",
    "@zeal/toolkit": "workspace:*",
    "@zeal/uikit": "workspace:*",
    "axios": "0.27.2",
    "react": "18.2.0",
    "react-dom": "18.1.0",
    "react-intl": "6.2.10",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-svg": "14.1.0",
    "react-native-web": "0.19.9",
    "typescript": "5.3.3",
    "viem": "2.0.0",
    "wagmi": "2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-transform-runtime": "7.22.15",
    "@babel/preset-env": "7.22.15",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.22.11",
    "@sentry/cli": "2.30.2",
    "@types/node": "20.11.29",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.23",
    "@zeal/eslint-config": "workspace:*",
    "babel-plugin-named-exports-order": "0.0.2",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "esbuild-loader": "3.0.1",
    "eslint": "8.3.0",
    "image-minimizer-webpack-plugin": "3.8.3",
    "imagemin": "8.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "10.0.1",
    "lint-staged": "13.0.1",
    "sass": "1.68.0",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.2",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "5.3.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0"
  }
}
